set title "http requests per day"
set output "data.png"

set terminal pngcairo transparent enhanced linewidth 2 font 'arial,10' size 600,400


;#set timefmt '%Y-%m-%d'
;#set xdata time
set format y '%.1s%c'
set xtics 0,100,100

set boxwidth 0.5
set style fill solid

set xrange [55.5:72.5]
set key off


set xtics rotate



set title "HTTP requests per day"
set output "requests.png"
plot "data2.csv" using 1:3:xtic(2) notitle with boxes

set title "unique IP addresses per day"
set output "uniques.png"
plot "data2.csv" using 1:5:xtic(2) notitle with boxes

